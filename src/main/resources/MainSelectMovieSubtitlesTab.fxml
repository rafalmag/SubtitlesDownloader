<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<TabPane xmlns:fx="http://javafx.com/fxml"
         fx:controller="pl.rafalmag.subtitledownloader.gui.FXMLMainSelectedMovieSubtitlesTabController">
    <Tab fx:id="selectMovieSubtitlesTab" text="%SelectSubtitles">
    <VBox>
        <TableView fx:id="table" VBox.vgrow="ALWAYS"/>
        <Label fx:id="selectedSubtitles" alignment="BOTTOM_LEFT" prefWidth="2500.0" text="%SelectedSubtitle"
               VBox.vgrow="SOMETIMES"/>
    </VBox>
</Tab>
</TabPane>
